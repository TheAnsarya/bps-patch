<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net10.0</TargetFramework>
		<RootNamespace>bps_patch</RootNamespace>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<EnablePreviewFeatures>false</EnablePreviewFeatures>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="bps-patch.Tests\**" />
		<EmbeddedResource Remove="bps-patch.Tests\**" />
		<None Remove="bps-patch.Tests\**" />
		<Compile Remove="bps-patch.Benchmarks\**" />
		<EmbeddedResource Remove="bps-patch.Benchmarks\**" />
		<None Remove="bps-patch.Benchmarks\**" />
	</ItemGroup>

	<ItemGroup>
		<InternalsVisibleTo Include="bps-patch.Tests" />
		<InternalsVisibleTo Include="bps-patch.Benchmarks" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="System.IO.Hashing" Version="9.0.0" />
	</ItemGroup>

</Project>
